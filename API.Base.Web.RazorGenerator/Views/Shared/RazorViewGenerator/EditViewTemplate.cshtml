@model API.Base.Web.RazorGenerator.Models.Edit.EditTemplateModel
@@model @Model.FullTypeName
@@{
ViewData["Title"] = "Edit " + Model.ToString();
}

<h4>@@ViewData["Title"]</h4>

<div class="row">
    <div class="col-12 col-sm-10 col-md-8 col-lg-7">
        @Html.Raw("<form asp-action=\"Edit\">")
        @Html.Raw("<partial name=\"_Form\"></partial>")
        <div class="form-group">
            <input type="submit" value="Save" class="btn btn-primary"/>
        </div>
        @Html.Raw("</form>")
    </div>
</div>
<div>
    <a href='@@Url.Action("Index")'>Back to list</a> | 
    <a href='@@Url.Action("Details", new { Id = Model.Id})'>Back to details</a>
</div>
